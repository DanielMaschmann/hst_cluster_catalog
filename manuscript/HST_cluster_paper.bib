	note = {ADS Bibcode: 2019ApJ...887...80K},
	keywords = {Astrophysics - Astrophysics of Galaxies, Spiral galaxies, H II regions, 1560, 391, Disk galaxies, 694, Metallicity, 574, Galaxy abundances, 1031, 224, Chemical abundances},
	pages = {80},
	file = {Full Text PDF:/home/benutzer/Zotero/storage/ZZPZLG4B/Kreckel et al. - 2019 - Mapping Metallicity Variations across Nearby Galax.pdf:application/pdf},
}

