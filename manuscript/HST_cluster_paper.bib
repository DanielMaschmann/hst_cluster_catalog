	keywords = {Astronomy, Magellanic Clouds, Star Clusters, Astronomical Catalogs, Globular Clusters, Stellar Spectrophotometry, Ubv Spectra, Milky Way Galaxy, Classifications, Stellar Systems},
	pages = {803--814},
