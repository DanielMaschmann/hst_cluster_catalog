	keywords = {847, Astrophysics - Astrophysics of Galaxies, Interstellar medium, Extinction, 1569, 836, Interstellar dust, Star formation, 1565, 412, 505, 842, Dust continuum emission, Interstellar filaments, Star forming regions},
	pages = {L13},
