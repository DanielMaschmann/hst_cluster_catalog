	note = {ADS Bibcode: 2018MNRAS.476.1942A},
	keywords = {Astrophysics - Astrophysics of Galaxies, galaxies: starburst, galaxies: star clusters: general, galaxies: abundances, galaxies: dwarf, galaxies: individual: NGC 4449, galaxies: irregular},
	pages = {1942--1967},
	file = {Full Text PDF:/home/benutzer/Zotero/storage/3UZDPZIJ/Annibali et al. - 2018 - LBTMODS spectroscopy of globular clusters in the .pdf:application/pdf},
