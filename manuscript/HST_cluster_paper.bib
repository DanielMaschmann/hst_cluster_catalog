	keywords = {Astrophysics, galaxies: interactions, galaxies: kinematics and dynamics, galaxies: ISM, galaxies: individual: NGC 4569},
	pages = {35--46},
	file = {Full Text PDF:/home/benutzer/Zotero/storage/7V725WXX/Vollmer et al. - 2004 - NGC 4569 Recent evidence for a past ram pressure .pdf:application/pdf},
}

