@article{gieles_distinction_2011,
	title = {The distinction between star clusters and associations},
	volume = {410},
	issn = {0035-8711},
	url = {https://ui.adsabs.harvard.edu/abs/2011MNRAS.410L...6G},
	doi = {10.1111/j.1745-3933.2010.00967.x},
	abstract = {In Galactic studies a distinction is made between (open) star clusters and associations. For barely resolved objects at a distance of several Mpc, this distinction is not trivial to make. Here we provide an objective definition by comparing the age of the stars to the crossing time of nearby stellar agglomerates. We find that a satisfactory separation can be made where this ratio equals unity. Stellar agglomerates for which the age of the stars exceeds the crossing time are bound, and are referred to as star clusters. Alternatively, those for which the crossing time exceeds the stellar age are unbound and are referred to as associations. This definition is useful whenever reliable measurements for the mass, radius and age are available.},
	urldate = {2023-10-02},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Gieles, Mark and Portegies Zwart, Simon F.},
	month = jan,
	year = {2011},
	note = {ADS Bibcode: 2011MNRAS.410L...6G},
	keywords = {galaxies: star clusters: general, Astrophysics - Galaxy Astrophysics, stars: formation, open clusters and associations: general},
	pages = {L6--L7},
	file = {Full Text:/home/benutzer/Zotero/storage/8GDJ9WII/Gieles and Portegies Zwart - 2011 - The distinction between star clusters and associat.pdf:application/pdf},
}

@article{fall_similarities_2012,
	title = {Similarities in {Populations} of {Star} {Clusters}},
	volume = {752},
	issn = {0004-637X},
	url = {https://ui.adsabs.harvard.edu/abs/2012ApJ...752...96F},
	doi = {10.1088/0004-637X/752/2/96},
	abstract = {We compare the observed mass functions and age distributions of star clusters in six well-studied galaxies: the Milky Way, Magellanic Clouds, M83, M51, and Antennae. In combination, these distributions span wide ranges of mass and age: 102 {\textless}{\textasciitilde} M/M ⊙ {\textless}{\textasciitilde} 106 and 106 {\textless}{\textasciitilde} τ/yr {\textless}{\textasciitilde} 109. We confirm that the distributions are well represented by power laws: dN/dMvpropM β with β ≈ -1.9 and dN/dτvpropτγ with γ ≈ -0.8. The mass and age distributions are approximately independent of each other, ruling out simple models of mass-dependent disruption. As expected, there are minor differences among the exponents at a level close to the true uncertainties, epsilonβ {\textasciitilde} epsilonγ {\textasciitilde} 0.1-0.2. However, the overwhelming impression is the similarity of the mass functions and age distributions of clusters in these different galaxies, including giant and dwarf, quiescent and interacting galaxies. This is an important empirical result, justifying terms such as "universal" or "quasi-universal." We provide a partial theoretical explanation for these observations in terms of physical processes operating during the formation and disruption of the clusters, including star formation and feedback, subsequent stellar mass loss, and tidal interactions with passing molecular clouds. A full explanation will require additional information about the molecular clumps and star clusters in galaxies beyond the Milky Way.},
	urldate = {2023-10-02},
	journal = {The Astrophysical Journal},
	author = {Fall, S. Michael and Chandar, Rupali},
	month = jun,
	year = {2012},
	note = {ADS Bibcode: 2012ApJ...752...96F},
	keywords = {Astrophysics - Astrophysics of Galaxies, galaxies: star clusters: general, stars: formation, galaxies: individual: Magellanic Clouds Milky Way M51 M83 NGC 4038/39},
	pages = {96},
	file = {Full Text PDF:/home/benutzer/Zotero/storage/MUCXNQG5/Fall and Chandar - 2012 - Similarities in Populations of Star Clusters.pdf:application/pdf},
}

