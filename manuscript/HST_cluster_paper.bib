	note = {ADS Bibcode: 2009ApJ...706..599L},
	keywords = {galaxies: evolution, galaxies: photometry, stars: formation, surveys, ultraviolet: galaxies, galaxies: dwarf, Astrophysics - Cosmology and Extragalactic Astrophysics},
	pages = {599--613},
