	note = {ADS Bibcode: 2016MNRAS.457.4296W},
	keywords = {Astrophysics - Astrophysics of Galaxies, galaxies: star clusters: general, galaxies: star formation, H II regions, stars: early-type, galaxies: individual: NGC 1566, NGC 5253},
	pages = {4296--4322},
