	month = jun,
	year = {2017},
	note = {ADS Bibcode: 2017ApJ...841...92R},
	keywords = {Astrophysics - Astrophysics of Galaxies, galaxies: star clusters: general, galaxies: general, Astrophysics - Solar and Stellar Astrophysics, galaxies: individual: NGC 628, NGC 1313},
