	keywords = {Astrophysics - Astrophysics of Galaxies, galaxies: star clusters: general, Astrophysics - Instrumentation and Methods for Astrophysics, I.2, I.5, Physics - Data Analysis, Physics - Instrumentation and Detectors, Statistics and Probability},
	pages = {3178--3193},
