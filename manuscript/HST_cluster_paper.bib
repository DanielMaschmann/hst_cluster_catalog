	month = oct,
	year = {2013},
	note = {ADS Bibcode: 2013A\&A...558A..53K},
	keywords = {Astrophysics - Astrophysics of Galaxies, galaxies: fundamental parameters, galaxies: star clusters: general, galaxies: photometry, Galaxy: stellar content, globular clusters: general, open clusters and associations: general},
	pages = {A53},
